SQL: INSERT INTO `ad_story_content` (`article_id`,`content`,`images`,`image_count`,`add_time`) VALUES ('4246780930','<p>[汽车之家 新车图解] 最近，本田车一股脑地迎来换代，思铂睿、奥德赛、飞度、CR-V，这次终于轮到锋范了。像刚列举的这些车一样，全新锋范向更帅的方向演变。在竞争对手们纷纷走年轻化设计路线的今天，全新锋范的外观对消费者将有更大的吸引力。（目前厂家未揭晓该车内饰，本篇文章仅为大家呈现全新锋范的外观部分。）</p><p><img src=\"http://p2.pstatp.com/large/3003/8456705089\" img_width=\"620\" img_height=\"684\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3002/8347392081\" img_width=\"620\" img_height=\"350\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3007/7910892562\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3006/8044454850\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3007/7910980086\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3005/8182739790\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3007/7911069707\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3007/7911209239\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3006/8044912675\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3001/8477155920\" img_width=\"620\" img_height=\"413\" alt=\"更锋芒、更有范儿 车展实拍本田锋范\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><i