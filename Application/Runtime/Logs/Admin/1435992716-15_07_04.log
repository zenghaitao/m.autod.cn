SQL: INSERT INTO `ad_story_content` (`article_id`,`content`,`images`,`image_count`,`add_time`) VALUES ('4246634019','<p>[汽车之家 新车图解] 在2015上海车展上，雷克萨斯推出了ES中期改款车型，该车除了外观造型较老款变得更为年轻与时尚外，内饰虽然造型变化不大，但通过对细节设计的提升与优化，使之整体精致感有了明显提升。而今天，作为老款雷克萨斯ES车主的我，就和大家来细聊一下新车的变化到底在哪里。</p><p><img src=\"http://p2.pstatp.com/large/3003/7031281241\" img_width=\"620\" img_height=\"684\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3003/7031423706\" img_width=\"620\" img_height=\"300\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3002/6947714002\" img_width=\"620\" img_height=\"665\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3031/7888651964\" img_width=\"620\" img_height=\"452\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3006/6628838674\" img_width=\"620\" img_height=\"776\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3004/6798332668\" img_width=\"620\" img_height=\"439\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/3007/6466799982\" img_width=\"620\" img_height=\"379\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p7.pstatp.com/large/3002/6948773981\" img_width=\"620\" img_height=\"561\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/3006/6629478855\" img_width=\"620\" img_height=\"450\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p1.pstatp.com/large/3004/6799384546\" img_width=\"620\" img_height=\"421\" alt=\"小改变更精致 车展实拍新款雷克萨斯ES\"  onerror=\"javascript:errorimg.call(this);\" >