SQL: INSERT INTO `ad_story_content` (`article_id`,`content`,`images`,`image_count`,`add_time`) VALUES ('4241885035','<p>[汽车之家 新车图解] 中期改款的CX-5首次亮相于去年11月份的洛杉矶车展，5个月后的今天，长安马自达把它的国产版本带到了我们面前，那国产新款CX-5到底有多大改进，它和海外版本车型有什么不同？我们一起看文章。</p><p><img src=\"http://p3.pstatp.com/large/2988/6099982689\" img_width=\"620\" img_height=\"684\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/2990/5882980756\" img_width=\"620\" img_height=\"300\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/2987/6266417183\" img_width=\"620\" img_height=\"465\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/2990/5883196323\" img_width=\"620\" img_height=\"604\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/2991/5827988416\" img_width=\"620\" img_height=\"721\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p1.pstatp.com/large/2990/5883451580\" img_width=\"620\" img_height=\"716\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p1.pstatp.com/large/2988/6100553545\" img_width=\"620\" img_height=\"758\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p3.pstatp.com/large/2988/6100716147\" img_width=\"620\" img_height=\"465\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p2.pstatp.com/large/2986/6339908925\" img_width=\"620\" img_height=\"442\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img src=\"http://p1.pstatp.com/large/2984/6333279703\" img_width=\"620\" img_height=\"564\" alt=\"外观小改/配置提升 实拍新款马自达CX-5\"  onerror=\"javascript:errorimg.call(this);\" ></p><p><img sr