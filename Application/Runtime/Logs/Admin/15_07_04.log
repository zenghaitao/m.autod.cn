SQL: INSERT INTO `ad_story_content` (`article_id`,`content`,`images`,`image_count`,`add_time`) VALUES ('3683637761','<p><h1>泊车技术哪家强？中国车手找韩岳</h1></p><p><strong>【啊车网 资讯】</strong>日前，中国车手韩岳再次将一项近乎不可能打破的纪录推向极限。在数天前举行的中国汽车飘移锦标赛上，韩岳驾驶一辆车身长3853mm的MINI三门掀背版，成功泊入间距只有3933mm的两辆MINI之间，以8cm的成绩刷新了吉尼斯世界纪录“间距最小的汽车飘移入位”称号，打破了由英国人Alastair Moffatt在2013年7月创造的8.6cm前纪录。</p>',NULL,'1','2015-07-04 19:10:49') [ RunTime:0.0005s ]
