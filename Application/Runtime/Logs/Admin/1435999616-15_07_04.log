SQL: INSERT INTO `ad_story_content` (`article_id`,`content`,`images`,`image_count`,`add_time`) VALUES ('4251605157','<p>第十六届上海国际车展的丰田汽车展台，一款来自“未来”的概念车型——丰田FV2吸引了众多观展者的眼球，借此这款最早亮相于13年东京车展的超未来智能概念车终于与中国观众有了相对近距离的接触。该车名称中的FV来自英文“Fun Vehicle”的缩写，意在实现丰田汽车针对这款新车而度身定制的“快乐驾驶哲学”。</p><img src=\"http://p3.pstatp.com/large/3025/6752324679\" img_width=\"608\" img_height=\"669\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p1.pstatp.com/large/3029/6408373572\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p2.pstatp.com/large/3024/6881583113\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p2.pstatp.com/large/3025/6752818567\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p1.pstatp.com/large/3028/6570611587\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p2.pstatp.com/large/3029/6408745307\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p1.pstatp.com/large/3029/6408858604\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p1.pstatp.com/large/3031/6014554307\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p2.pstatp.com/large/3024/6881793541\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p1.pstatp.com/large/3026/6490162823\" img_width=\"608\" img_height=\"456\" alt=\"驾驭未来 上海车展实拍丰田FV2概念车\"  onerror=\"javascript:errorimg.call(this);\" ><img src=\"http://p3.pstatp.com/large/3026/6490307917\"